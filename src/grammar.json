    },
    "promise_guard": {
      "type": "PATTERN",
      "value": "[a-zA-Z_]+:"
    },
    "class_guard": {
      "type": "PATTERN",
      "value": "[.|&!()a-zA-Z0-9_:][\\t .|&!()a-zA-Z0-9_:]*::"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "macro": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "@[^\\n].*"
      }
    }
