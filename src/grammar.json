      "type": "PATTERN",
      "value": "\\\"((\\\\(.|\\n))|[^\"\\\\])*\\\"|\\'((\\\\(.|\\n))|[^'\\\\])*\\'|`[^`]*`"
    },
    "identifier": {
      "type": "PATTERN",
